{
  "PORT": "$PORT::number",
  "REFRESH_TOKEN_SECRET": "$REFRESH_TOKEN_SECRET::string",
  "ACCESS_TOKEN_SECRET": "$ACCESS_TOKEN_SECRET::string",
  "PASSWORD_SECRET": "$PASSWORD_SECRET::string",
  "MONGO_DB": {
    "CONNECTION_RETRIES": 2,
    "CONNECTION_RETRY_TIMEOUT": 1000,
    "CONNECTION_URL": "$MONGO_CONNECTION_URL::string"
  },
  "RATE_LIMITING": {
    "AUTH": {
      "REGISTER": {
        "THRESHOLD": 5,
        "WINDOW": 5000
      },
      "LOGIN": {
        "THRESHOLD": 5,
        "WINDOW": 5000
      },
      "LOGOUT": {
        "THRESHOLD": 5,
        "WINDOW": 5000
      }
    }
  },
  "AUTH": {
    "MAX_FAILED_LOGINS": 5,
    "LOCKOUT_TIME": 5,
    "FAILED_LOGIN_ATTEMPTS_WINDOW": 5,
    "ACCESS_TOKEN_LIFETIME": "1d",
    "IDLE_TIMEOUT": 5000
  }
}