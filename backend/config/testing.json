{
  "ENV": "$NODE_ENV::string",
  "PORT": "$PORT::number",
  "REFRESH_TOKEN_SECRET": "$REFRESH_TOKEN_SECRET::string",
  "ACCESS_TOKEN_SECRET": "$ACCESS_TOKEN_SECRET::string",
  "PASSWORD_SECRET": "$PASSWORD_SECRET::string",
  "RETRY_LIMIT": 2,
  "MONGO_DB": {
    "CONNECTION_URL": "$MONGO_CONNECTION_URL::string"
  },
  "REDIS": {
    "HOST": "$REDIS_HOST::string",
    "PORT": "$REDIS_PORT::number",
    "PASSWORD": "$REDIS_PASSWORD::string"
  },
  "FIREBASE": {
    "PROJECT_ID": "$FIREBASE_PROJECT_ID::string",
    "CLIENT_EMAIL": "$FIREBASE_CLIENT_EMAIL::string",
    "PRIVATE_KEY": "$FIREBASE_PRIVATE_KEY::string"
  },
  "RATE_LIMITING": {
    "USER": {
      "POST": {
        "THRESHOLD": 5,
        "WINDOW": 5000
      },
      "PUT": {
        "THRESHOLD": 5,
        "WINDOW": 5000
      },
      "GET": {
        "THRESHOLD": 5,
        "WINDOW": 5000
      },
      "DELETE": {
        "THRESHOLD": 5,
        "WINDOW": 5000
      }
    },
    "HEALTH_CHECK": {
      "GET": {
        "THRESHOLD": 5,
        "WINDOW": 5000
      }
    }
  },
  "AUTH": {
    "MAX_FAILED_LOGINS": 5,
    "LOCKOUT_TIME": 5,
    "FAILED_LOGIN_ATTEMPTS_WINDOW": 5,
    "ACCESS_TOKEN_LIFETIME": "1d",
    "IDLE_TIMEOUT": 5000
  }
}