{
  "PORT": "$PORT::number",
  "REFRESH_TOKEN_SECRET": "$REFRESH_TOKEN_SECRET::string",
  "ACCESS_TOKEN_SECRET": "$ACCESS_TOKEN_SECRET::string",
  "PASSWORD_SECRET": "$PASSWORD_SECRET::string",
  "MONGO_DB": {
    "CONNECTION_RETRIES": 2,
    "CONNECTION_RETRY_TIMEOUT": 1000,
    "CONNECTION_URL": "$MONGO_CONNECTION_URL::string"
  },
  "RATE_LIMITING": {
    "AUTH": {
      "REGISTER": {
        "THRESHOLD": 100,
        "WINDOW": 3600000
      },
      "LOGIN": {
        "THRESHOLD": 100,
        "WINDOW": 3600000
      },
      "LOGOUT": {
        "THRESHOLD": 100,
        "WINDOW": 3600000
      }
    }
  },
  "AUTH": {
    "MAX_FAILED_LOGINS": 10,
    "LOCKOUT_TIME": 900,
    "FAILED_LOGIN_ATTEMPTS_WINDOW": 900,
    "ACCESS_TOKEN_LIFETIME": "15m",
    "IDLE_TIMEOUT": 604800
  },
  "TRUSTED_IPS": "$TRUSTED_IPS::set:string"
}